def Vector2
    x y
impl
    fun add(other)
        Vector2(self.x + other.x,
                self.y + other.y)
    end
    
    fun neg()
        Vector2(-self.x, -self.y)
    end
end

def Vector3
    x y z
end

let v = Vector2(3, 4)
v += Vector3(4, 5, 6)
print(v)
print(-v)

